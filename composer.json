{
    "name": "scorelabs/laravel-event-fake",
    "description": "An enhanced Event::fake for Laravel event testing",
    "license": "MIT",
    "keywords": ["laravel event", "laravel testing"],
    "authors": [
        {
            "name": "Brandon Shar",
            "email": "bshar@scorelabs.org"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "^5.0",
        "orchestra/testbench": "3.6"
    },
    "require-dev": {
        "php-mock/php-mock-mockery": "^1.1",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "ScoreLabs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Event": "ScoreLabs\\Event"
            }
        }
    },  
    "scripts": {
        "test": [
            "rm vendor -rf; rm composer.lock; echo 1",
            "composer require orchestra/testbench 3.6",
            "phpunit | tee phpunit.6.log",
            "rm vendor -rf; rm composer.lock; echo 1",
            "composer require orchestra/testbench 3.5",
            "phpunit | tee phpunit.5.log",
            "rm vendor -rf; rm composer.lock; echo 1",
            "composer require orchestra/testbench 3.4",
            "phpunit | tee phpunit.4.log",
            "rm vendor -rf; rm composer.lock; echo 1",
            "composer require orchestra/testbench 3.3",
            "phpunit | tee phpunit.3.log",
            "rm vendor -rf; rm composer.lock; echo 1",
            "cat phpunit.*.log"
        ]
    }
}
